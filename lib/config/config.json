{
  "joi": {
    "validatorConfig": {
      "abortEarly": false,
      "convert": false,
      "allowUnknown": true,
      "skipFunctions": true,
      "stripUnknown": false,
      "presence": "optional",
      "noDefaults": false
    }
  },
  "es": {
    "url": "http://localhost:9200",
    "requestTimeout": 120000,
    "maxResults": 10000,
    "scrollTimeout": "5s",
    "retryOnConflict": 10,
    "retries":  {
      "base": 2.0,
      "maxRetries": 5
    }
  },
  "logger": {
    "name": "elasticsearch",
    "streams": [
      {
        "level": "info",
        "type": "console"
      }
    ]
  }
}
