{
  "warning": {
    "downloadedB": 0,
    "searchCalls": 0
  },
  "joi": {
    "validatorConfig": {
      "abortEarly": false,
      "convert": false,
      "allowUnknown": true,
      "skipFunctions": true,
      "stripUnknown": false,
      "presence": "optional",
      "noDefaults": false
    }
  },
  "es": {
    "url": "http://localhost:9200",
    "requestTimeoutSeconds": 120,
    "pingTimeoutSeconds": 5,
    "maxRetries": 3,
    "maxResults": 10000,
    "paginationSize": 1000,
    "pitTimeoutSeconds": 60,
    "retryOnConflict": 10,
    "retries":  {
      "base": 2.0,
      "maxRetries": 6
    }
  },
  "logger": {
    "name": "elasticsearch",
    "streams": [
      {
        "level": "warn",
        "type": "console"
      }
    ]
  }
}
